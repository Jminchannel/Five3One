    <!-- Training Templates -->
    <string name="template_classic_4day">经典四天分化</string>
    <string name="template_classic_4day_desc">每个主项拥有专属训练日，专注度最高，恢复效果最好。是Jim Wendler最推荐的原始版本。</string>
    
    <string name="template_3day">三天全身训练</string>
